{
    "name": "vipertecpro/vimeo",
    "description": "A Vimeo bridge for Laravel",
    "license": "Apache-2.0",
    "keywords": ["laravel","laravel-6","laravel-7", "vimeo", "video"],
    "authors": [
        {
            "name": "Vipul Walia",
            "email": "vipertecpro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "graham-campbell/manager": "^4.0",
        "vimeo/vimeo-api": "^3.0.3"
    },
    "require-dev": {
        "laravel/framework": "^5.8|^6.0|^7.0",
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^5.2",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "psalm/plugin-laravel": "^0.4",
        "laravel/lumen-framework": "^5.8|^6.0|^7.0",
        "vimeo/psalm": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Vipertecpro\\Vimeo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vipertecpro\\Tests\\Vimeo\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vipertecpro\\Vimeo\\VimeoServiceProvider"
            ],
            "aliases": {
                "Vimeo": "Vipertecpro\\Vimeo\\Facades\\Vimeo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpunit": "phpunit",
        "tests": [
            "composer phpunit"
        ]
    }
}
